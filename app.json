{
  "expo": {
    "name": "Opar Takip",
    "slug": "OparTakip",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f8f8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.eryaz.oparasd",
      "supportsTablet": true
    },
    "android": {
      "permissions": [],
      "versionCode": 7,
      "package": "com.eryaz.oparasd"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}